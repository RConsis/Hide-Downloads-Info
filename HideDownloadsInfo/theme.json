{
  "name": "Hide Downloads Info",
  "version": "v1.0",
  "author": "RConsis",
  "manifest_version": 9,
  "id": "dfg4tgs43-h54g-gf44-gg43-dfg4fgs",
  "inject": {
    "shared.css": [
      "bigpicture"
    ]
  },
  "patches": {
    "Hide Nav Bar Icon": {
      "type": "checkbox",
      "values": {
        "No": {
          "--rconsis-hide-icon": [
            "flex",
            "bigpicture"
          ]
        },
        "Yes": {
          "--rconsis-hide-icon": [
            "none",
            "bigpicture"
          ]
        }
      }
    },
    "Hide Progress Bar": {
      "type": "checkbox",
      "values": {
        "No": {
          "--rconsis-hide-bar": [
            "flex",
            "bigpicture"
          ]
        },
        "Yes": {
          "--rconsis-hide-bar": [
            "none",
            "bigpicture"
          ]
        }
      }
    }
  }
}
